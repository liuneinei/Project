
.app{
    width: 100%;
    /*position: relative;*/
    margin: 0 auto;
    height: 100%;
    font-size: 14px;
}

.types {
    width: 90%;
    margin:0 auto;
    margin-top: 30px;
    display: flex;
    flex-wrap:wrap;
}
.types .types-item{
    display: inline-block;
    width: 32%;
    line-height: 60rpx;
    text-align: center;
    cursor: pointer;
    font-size: 30rpx;
    margin-bottom: 8rpx;
    border-radius: 5rpx;
    margin-right: 5rpx;
}
.types .types-item.ative{
    background: #71509e;
    color: #ffffff;
}
.input-list{
    width: 92%;
    margin: 0 auto;
    margin-top: 30px;
}
.input-list .input-item{
    margin-top: 20px;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.input-list .ipt{
    border: 1px solid #dddddd;
    width: 500rpx; 
    height: 30px;
    font-size: 30rpx;
    padding-left: 10rpx;
    /* margin-left: 10px; */
}

.input-list .w71{
    width: 170rpx;
}

.p15{
    padding:30rpx 30rpx 0 30rpx;
}
.warp{
  width: 92%;
  margin: 40rpx auto;
}
.box-image{  
    overflow: hidden;
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
}
.box-image .box{
    width: 335rpx;
    height: 200rpx;
    margin-right:10rpx;
    margin-top: 5rpx;
    border: 2rpx solid #ccc;
}
.box-image .cert-img{
    width: 100%;
    height: 200rpx;
}
.box-image .box:last-child{
  position: relative;
}
.box-image .box:nth-child(1n+2){
  margin-right: 0px;
}
.box-image .box:last-child .cert-img{
  height: 100rpx;
  width: 100rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -50rpx;
  margin-top: -50rpx;
}
.btn{
    width: 83px;
    height: 25px;
    text-align: center;
    border: 1px solid #dddddd;
    background: none;
}
.form-btn{
    margin-top: 20px;
    text-align: center
}
.form-btn .btn{
    height: 35px;
    line-height:35px;
    font-size: 32rpx;
}
.btn-next{
  height: 70rpx;
  line-height: 70rpx;
  width: 500rpx;
  background: #71509e;
  border: 1px solid #71509e;
  color: #fff;
  font-size: 28rpx;
}

/*相册；Begin*/
.swiper-container {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #000;
    z-index:99;
}

.swiper-container image {
    width: 100%;
    height: 100%;
}

.btn-del{
  width: 100rpx;
  color: #ffffff;
  position: fixed;
  bottom: 35rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
    display: inline-block;
    vertical-align: middle;
    font: normal normal normal 14px/1 "weui";
    font-size: inherit;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    text-align: center;
}
.btn-del::before{
  content: "\EA11";
      display: inline-block;
}
/*相册：End*/